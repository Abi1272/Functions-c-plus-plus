#include<iostream>
using namespace std;

void MiniCalc(int one, int two);

int main() {
	int L1, L2;
	cout << "enter two numbers : " << endl;
	cin >> L1;
	cin >> L2;
	MiniCalc(L1, L2);
}

void MiniCalc(int one, int two) {
	cout << "added:" << one + two << endl;
	cout << "subtracted:" << one - two << endl;
	cout << "divided:" << one / two << endl;
	cout << "multiplied:" << one * two << endl;

}
